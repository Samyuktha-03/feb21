assign-1
n=int(input("enter number"))
ascii_value=97
for i in range(1,n+1):
    for j in range(1,n+1):
        print(chr(ascii_value),end=" ")
        ascii_value+=1
    print("\n")

assign-2
n=int(input("enter n value"))
for i in range(n+1,0,-1):
    for j in range(0,i-1):
      print("*",end=" ")
    print(" ")
